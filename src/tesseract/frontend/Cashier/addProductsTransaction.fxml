<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="390.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/16" fx:controller="tesseract.frontend.Cashier.AddProductsTransactionController">
    <stylesheets>
        <URL value="@cashierui.css" />
    </stylesheets>
   <children>
      <HBox alignment="CENTER" layoutY="24.0" prefHeight="91.0" prefWidth="617.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Added Successfully!" textAlignment="CENTER">
               <font>
                  <Font name="Century Gothic" size="40.0" />
               </font>
            </Text>
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutY="108.0" prefHeight="178.0" prefWidth="617.0">
         <children>
            <ImageView fitHeight="178.0" fitWidth="201.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@Product%20Pictures/Check.png" />
               </image>
            </ImageView>
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutX="17.0" layoutY="276.0" prefHeight="91.0" prefWidth="600.0">
         <children>
            <Button fx:id="addNewProduct" mnemonicParsing="false" prefHeight="48.0" prefWidth="174.0" styleClass="newCustomerBtn" stylesheets="@cashierui.css" text="Add New Product">
               <font>
                  <Font size="17.0" />
               </font>
               <HBox.margin>
                  <Insets left="10.0" top="5.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="ProceedToTransaction" mnemonicParsing="false" prefHeight="48.0" prefWidth="209.0" styleClass="newCustomerBtn" stylesheets="@cashierui.css" text="Proceed to Transaction">
               <font>
                  <Font size="17.0" />
               </font>
               <HBox.margin>
                  <Insets left="10.0" top="5.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
